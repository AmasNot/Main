local gameID = game.GameId

local scripts = {
    [2753915549] = "https://raw.githubusercontent.com/AmasNot/Game/refs/heads/main/BloxFruits.Lua", -- Blox Fruits Sea 1
    [4442272183] = "https://raw.githubusercontent.com/AmasNot/Game/refs/heads/main/BloxFruits.Lua", -- Blox Fruits Sea 2
    [7449423635] = "https://raw.githubusercontent.com/AmasNot/Game/refs/heads/main/BloxFruits.Lua", -- Blox Fruits Sea 3
    [6884266247] = "https://raw.githubusercontent.com/AmasNot/Game/refs/heads/main/untitled%20drill%20game.Lua", -- untitled drill game
    [7074860883] = "https://raw.githubusercontent.com/Saiky988/Arise/refs/heads/main/AriseSaryn.lua" -- Arise Crossover
}

if scripts[gameID] then
    loadstring(game:HttpGet(scripts[gameID]))()
else
    local player = game.Players.LocalPlayer
    player:Kick("[Not Supported Game] ID: " .. tostring(gameID))
end
